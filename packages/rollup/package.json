{
  "name": "@timsexperiments/rollup",
  "private": true,
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsc && rollup src/index.ts --file dist/index.mjs --format es -p rollup-plugin-esbuild"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "tsconfig": "workspace:^"
  },
  "dependencies": {
    "glob": "^10.3.4",
    "rollup": "^3.29.2",
    "rollup-plugin-esbuild": "^5.0.0"
  }
}
